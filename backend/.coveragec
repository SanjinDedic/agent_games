[run]
omit =
    legacy_agents/*
    test/*
    local_sim.py
    production_database_setup.py
    create_5_test_players.py
    local_sim_api.py
    standalone_game.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if __name__ == .__main__.:
    raise NotImplementedError
    pass
    except ImportError:
    def __str__